%% streamweave: begin
%% streamweave: input configuration -> keys.configuration
%% streamweave: input input -> keys.in
%% streamweave: output output <- keys.out
%% streamweave: output error <- keys.error
%% streamweave: node keys kind=ObjectKeysNode
%% streamweave: end
%% ---
%% Style B: The node itself is a subgraph; its ports are boxes inside the group.
%% External I/O nodes sit outside and connect to the port boxes on the boundary.
%% ---
flowchart TD
  configuration["configuration"]
  input["input"]
  output["output"]
  error["error"]

  subgraph keys["ObjectKeysNode"]
    direction LR
    keys_in_configuration["configuration"]
    keys_in_in["in"]
    keys_core["keys"]
    keys_out_output["output"]
    keys_out_error["error"]
    keys_in_configuration --> keys_core
    keys_in_in --> keys_core
    keys_core --> keys_out_output
    keys_core --> keys_out_error
  end

  configuration --> keys_in_configuration
  input --> keys_in_in
  keys_out_output --> output
  keys_out_error --> error
